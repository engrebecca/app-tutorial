// Config.json stores details about the environment the database will run in. 
// The information stored in the development, test, and production objects will be used to create a connection to the correct database depending on the environment.
{
  "development": {
    "username": "root",
    "password": "password",
    "database": "passport_demo",
    "host": "127.0.0.1",
    "dialect": "mysql"
  },
  // Test refers to the database that will be used for testing, if there is one.
  "test": {
    "username": "root",
    "password": null,
    "database": "database_test",
    "host": "127.0.0.1",
    "dialect": "mysql"
  },
  // Production refers to the database that will be used for production.
  "production": {
    "username": "root",
    "password": null,
    "database": "database_production",
    "host": "127.0.0.1",
    "dialect": "mysql"
  }
}